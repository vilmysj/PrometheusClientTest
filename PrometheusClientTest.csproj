<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>PrometheusClientTest</RootNamespace>
    <AssemblyName>PrometheusClientTest</AssemblyName>
    <TargetFramework>net462</TargetFramework>
    <Configurations>Debug;Release;</Configurations>
    <OutputType>Exe</OutputType>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Microsoft.Owin.Host.HttpListener" Version="3.1.0" />
    <PackageReference Include="Microsoft.Owin.Hosting" Version="3.1.0" />
    <PackageReference Include="Prometheus.Client.Owin" Version="3.0.0" />
  </ItemGroup>
  
  <ItemGroup>
    <None Include="App.config">
      <TransformOnBuild>true</TransformOnBuild>
      <SubType>Designer</SubType>
    </None>
  </ItemGroup>

</Project>
